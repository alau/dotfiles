partial alphanumeric_keys
xkb_symbols "switch_case" {
  replace key <AD01> {
    [ colon, semicolon ]
  };
};

partial alphanumeric_keys
xkb_symbols "swedish_characters" {
  key <AC01> {
    symbols = [ a, A, adiaeresis, Adiaeresis ]
  };
  key <AC02> {
    symbols = [ o, O, odiaeresis, Odiaeresis ]
  };
  key <AD01> {
    symbols = [ semicolon, colon, aring, Aring ]
  };
};

partial alphanumeric_keys
xkb_symbols "relocate_equals" {
  key <AE04> {
    symbols = [ equal, 3 ]
  };
  key <AE06> {
    symbols = [ braceright, 9 ]
  };
};

partial alphanumeric_keys
xkb_symbols "compose" {
  key <LSGT> {
    symbols = [ yacute,  Multi_key ]
  };
};

partial alphanumeric_keys
xkb_symbols "menu_as_super" {
  key <MENU> {
    symbols = [ Super_R ]
  };
};

